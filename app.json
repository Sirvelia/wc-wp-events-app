{
  "expo": {
    "name": "WC & WP Events",
    "slug": "wc-wp-events-app",
    "version": "1.7.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wcwpeventsapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.sirvelia.wcwpeventsapp",
        "config": {
          "usesNonExemptEncryption": false
        },
        "infoPlist": {
          "CFBundleLocalizations": [
            "en",
            "es"
          ],
          "NSCameraUsageDescription": "This app needs access to your camera to scan QR codes for contact sharing.",
          "NSContactsUsageDescription": "This app needs access to your contacts to save scanned contact information."
        }
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/images/splash-icon.png",
          "backgroundColor": "#2c90ce"
        },
        "edgeToEdgeEnabled": true,
        "package": "com.sirvelia.wcwpeventsapp",
        "permissions": [
          "CAMERA",
          "READ_CONTACTS",
          "WRITE_CONTACTS"
        ]
      },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "buildToolsVersion": "35.0.0",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-notifications",
      "expo-localization",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": [
              "en",
              "es"
            ],
            "android": [
              "en",
              "es"
            ]
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to scan QR codes."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts to save scanned contact information."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "04631bdd-cf01-4320-8cc3-a560df08ccad"
      }
    },
    "owner": "sirvelia-labs"
  }
}
